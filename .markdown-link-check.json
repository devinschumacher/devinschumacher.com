{
  "ignorePatterns": [
    { "pattern": "^http://localhost" },
    { "pattern": "^#" },
    { "pattern": "^https://openai.com" },
    { "pattern": "github.com/user-attachments" },
    { "pattern": "private-user-images.githubusercontent.com" }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/(?!http)",
      "replacement": "https://devinschumacher.com/"
    }
  ],
  "timeout": "15s",
  "retryOn429": true,
  "retryCount": 2,
  "fallbackRetryDelay": "20s",
  "aliveStatusCodes": [200, 301, 302, 307, 308, 403]
}